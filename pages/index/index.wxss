
.indexContainer {
  display: flex;
  /* 竖轴排列 */
  flex-direction: column;
  /* 侧轴居中 */
  align-items: center;
  /* justify-content: space-between; */
  background: #4bcffa;
  height: 100%;
}

/* 头像 注意单位要使用响应式单位 */
.avatarUrl {
  width: 200rpx;
  height: 200rpx;
  border-radius: 50%;
  margin: 100px 0;
}

/* 用户名 */
.userNmae {
  font-size: 32rpx;
  font-weight: bold;
  margin: 100rpx 0;
}

.goStydy {
  border: 1px solid black;
  width: 300rpx;
  height: 80rpx;
  text-align: center;
  line-height: 80rpx;
  border-radius: 10rpx;
}